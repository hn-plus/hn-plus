{
    "name": "hn-plus",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Hacker News Plus",
    "background": {
        "page": "background.html"
    },
    "content_scripts": [ {
        "all_frames": true,
        "css" : [
            "css/content.css"
        ],
        "js": [
            "js/jquery.js",
            "js/content.js"
        ],
        "matches": [
            "https://news.ycombinator.com/*"
        ],
        "run_at": "document_end"
    } ]
}
