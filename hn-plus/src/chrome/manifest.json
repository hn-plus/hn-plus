{
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "css/content.css"
      ],
      "js": [
        "js/jquery.js",
        "js/content.js"
      ],
      "matches": [
        "https://news.ycombinator.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Hacker News Plus",
  "manifest_version": 2,
  "name": "hn-plus",
  "version": "1.0.0"
}
